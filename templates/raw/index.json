    "testimonials": {
      "type": "testimonials",
      "blocks": {
        "testimonial_024704": {
          "type": "testimonial",
          "settings": {
            "description": "<p>From the quality of the tents to the reliability of the backpacks, every piece of equipment was top-notch and well-maintained. The staff were incredibly knowledgeable, helping me pick out exactly what I needed for my weekend hike.</p>",
            "name": "John Mitchell",
            "position": "Portland",
            "stars": "5"
          },
          "disabled": null
        },
        "testimonial_024706": {
          "type": "testimonial",
          "settings": {
            "description": "<p>I was planning a last-minute kayaking trip, and they had everything I needed – from the kayak itself to life vests and dry bags. The rental process was simple and affordable, and the gear was in perfect condition.</p>",
            "name": "Emily Rodriguez",
            "position": "Austin",
            "stars": "4"
          },
          "disabled": null
        }
      },
