      "settings": {
        "background_image": "",
        "button_label": "MORE ABOUT US",
        "button_url": "booqable://root",
        "color_scheme": "set-3",
        "description": "<p>At our trailer rental business, we provide a diverse fleet of high-quality trailers designed to meet the demands of professionals and businesses. Whether you need a durable cargo trailer for transporting goods or a specialized trailer for unique projects, we offer reliable solutions to ensure your operations run smoothly. With flexible rental options and exceptional customer service, we’re committed to supporting your success, every step of the way.</p>",
        "full_width": false,
        "image": "booqable://assets/image-about-us-avatar.jpg",
        "image_circle": false,
        "padding_bottom": "small",
        "padding_bottom_mobile": "small",
        "padding_top": "small",
        "padding_top_mobile": "small",
        "show_borders": true,
        "tag": "MEET OUR TEAM",
        "title": "Our story"
      },
      "disabled": null
